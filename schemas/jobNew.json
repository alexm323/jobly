{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "integer"
        },
        "companyHandle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        }
    },
    "additionalProperties": true,
    "required": [
        "title",
        "companyHandle"
    ]
}